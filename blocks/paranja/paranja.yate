func nb-paranja(nodeset options) {
    default = {
        'theme': 'dark'
    }

    nb-block('paranja', nb-extend(default, options))
}

match .paranja nb {
    <div class="nb-paranja nb-paranja_theme_{ .theme }" data-nb='paranja'>
        apply . nb-main-attrs
        
        html( .content )
    </div>
}
