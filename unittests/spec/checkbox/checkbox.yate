 match .checkbox{

  nb-checkbox({
        "value": "my-value"
        "checked": true()
        "content": "Button"
        "id": "checkbox-checked"
    })

    nb-checkbox({
        "value": "my-value"
        "disabled": true()
        "content": "Button"
        "id": "checkbox-disabled"
    })

     nb-checkbox({
        "value": "my-value"
        "content": "Button"
        "id": "checkbox"
    })
}