                'href': '#'
                'text': 'пункт2'
            }
            {
                'href': '#'
                'text': 'пункт3'
            }
        ]
    }

    modal = {
        'id': 'popup4-jq'
        'modal': true()
        'menu': [
            {
                'href': '#'
                'text': 'пункт1'
            }
            {
                'href': '#'
                'text': 'пункт2'
            }
            {
                'href': '#'
                'text': 'пункт3'
            }
        ]
    }

    toRight = {
        'id': 'popup1-jq'
        'menu': [
            {
                'href': '#'
                'text': 'пункт1'
            }
            {
                'href': '#'
                'text': 'пункт2'
            }
            {
                'href': '#'
                'text': 'пункт3'
            }
        ]
    }

    toLeft = {
        'id': 'popup2-jq'
        'menu': [
            {
                'href': '#'
                'text': 'пункт1'
            }
            {
                'href': '#'
                'text': 'пункт2'
            }
            {
                'href': '#'
                'text': 'пункт3'
            }
        ]
    }

    toTop = {
        'id': 'popup3-jq'
        'menu': [
