match .toggler {
  nb-toggler({
        "id": "toggler"
        "attrs": {
            "value": "value"
        }
    })

    nb-toggler({
        "disabled": true()
        "id": "toggler-disabled"
    })

    nb-toggler({
        "checked": true()
        "id": "toggler-checked"
    })

 nb-toggler({
        "checked": true()
        "id": "toggler-tabindex"
        "tabindex": "1"
    })

}
